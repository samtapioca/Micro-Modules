#include <stdio.h>
int main()
{
 
  int x[] = {12,45,36,74,6};
  int largest = x[0];
  int smallest = x[0];
  int i;

  for(i=0;i<5;i++)
  {
    if(x[i]>largest)
      largest = x[i];
    if(x[i]<smallest)
      smallest = x[i];
  }
  printf("Largest is %d and smallest is %d\n",largest,smallest);
  return 0;
}
