#include <stdio.h>

int main(void) {
  int a, b, c, d;
  // To read four numbers in the pattern of an IP address
  // use the following code:
  scanf("%d.%d.%d.%d", &a, &b, &c, &d);

  // write your solution below  

  if (a >= 0 && a <= 225) {
      if (b >= 0 && b <=225) {
        if (c >= 0 && c <= 225) {
          if (d >= 0 && d <= 225) {
            printf("Valid");
          }
        }
      }
  }
   else {
    printf("Invalid");
  }      
  return 0;
}
