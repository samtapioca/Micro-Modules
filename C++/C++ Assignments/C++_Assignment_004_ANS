#include <iostream>
using namespace std;

int main() {

  string lyric1 = "Are you sleeping?";
  string lyric2 = "Brother John";
  string lyric3 = "Morning bells are ringing.";
  string lyric4 = "Ding, dong, ding.";

  cout << lyric1 << endl;
  cout << lyric1 << endl;
  cout << lyric2 << "," << endl;
  cout << lyric2 << "?" << endl;
  cout << lyric3 << endl;
  cout << lyric3 << endl;
  cout << lyric4 << endl;
  cout << lyric4 << endl;
  
}
