import java.util.Scanner;

class Main{
  public static void main (String[] args)
  {   
      int product = 0;
      Scanner n = new Scanner(System.in);
      int[] num = new int[5];

      for (int i = 0; i < 5; i++)
      {
          
          System.out.print("Enter a number: ");
          num[i] = n.nextInt();
          product = product + num[i];
          
      }
      System.out.println("Product: " + product);
  }
}
