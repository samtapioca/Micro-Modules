import java.io.*;

class Main {

 public static void main( String args[]) {
    try {
    FileWriter fw = new FileWriter("Chapter10.txt");
    PrintWriter pw = new PrintWriter(fw);

    pw.println("Hello, Chapter 10!");

    pw.close();
    }
    catch(IOException e) {
    System.out.print("File not found!");
    } 
  }
}
