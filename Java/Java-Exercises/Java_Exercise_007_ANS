class Main {
  public static void main(String[] args) {

    String name = "John" ;
    int age = 23;
    double height = 5.6;
    char initial = 'G';
    boolean student = true;

    System.out.println(name);
    System.out.println(age);
    System.out.println(height);
    System.out.println(initial);
    System.out.println(student);
  }
}
