import java.util.Scanner;
 
class Main {
  public static void main(String[] args) {
    Scanner n = new Scanner(System.in);
    String ans;
 
    System.out.print("Do you have a pet dog? ");
    ans = n.nextLine();

    if (ans.equals("Yes")) {
      System.out.print("What's your dog's name? ");
      ans = n.nextLine();
      System.out.print("Your dog's name is: " + ans);
    }
    else if (ans.equals("No")) {
      System.out.print("You must be a cat person.");
    }

 
  }
}
