import java.io.*;
 
class Main {
 
 public static void main( String args[]) {
    try {
    FileReader fr = new FileReader("Congratulations.txt");
    BufferedReader br = new BufferedReader(fr);
 
    String str = br.readLine();
 
    System.out.println(str);
 
    br.close();
    }
    catch(IOException e) {
    System.err.print("File not found!");
    } 
  }
}
