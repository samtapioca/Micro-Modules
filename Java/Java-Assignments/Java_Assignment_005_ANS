import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    Scanner input = new Scanner(System.in);
    String msg;

    System.out.println("Enter message: ");
    msg = input.nextLine();

    if (msg.length() <=6) {
       System.out.println("Insufficient Characters");
    }
    else {
    System.out.println(msg);
    }
  }
}
