import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    Scanner input = new Scanner(System.in);
    int num;

    System.out.println("Enter a number:");
    num = input.nextInt();

     switch(num%2)
    {
        case 0:
            System.out.println("EVEN");
            break;
        case 1:
            System.out.println("ODD");
            break;
    }
  }
}
