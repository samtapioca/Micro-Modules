import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int x;

        for (x = 1; x <= 3; x++) { 

            System.out.println("1**");

            for (x = 1; x <= 2; x++) {

                System.out.println("12*");

                for (x = 1; x <= 1; x++) {
                    System.out.println("123");
    
                }
            }
        }
    }
}

